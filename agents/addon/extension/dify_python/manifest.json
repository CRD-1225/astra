{
  "type": "extension",
  "name": "dify_python",
  "language": "python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "rte_runtime_python",
      "version": "0.4.0"
    }
  ],
  "api": {
    "property": {
      "user_id": {
        "type": "string"
      },
      "api_key": {
        "type": "string"
      },
      "base_url": {
        "type": "string"
      },
      "greeting": {
        "type": "string"
      },
      "failure_info": {
        "type": "string"
      },
      "max_history": {
        "type": "int64"
      }
    },
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ],
    "cmd_in": [
      {
        "name": "flush"
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      }
    ]
  }
}